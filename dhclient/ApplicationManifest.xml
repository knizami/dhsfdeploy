<?xml version="1.0" encoding="utf-8"?>
<ApplicationManifest ApplicationTypeName="dhMobile" ApplicationTypeVersion="1.0.0" 
    xmlns="http://schemas.microsoft.com/2011/01/fabric" 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Parameters>
        <Parameter Name="dhclient_InstanceCount" DefaultValue="2" />
    </Parameters>
    <ServiceManifestImport>
        <ServiceManifestRef ServiceManifestName="dhclientPkg" ServiceManifestVersion="1.0.0" />
    </ServiceManifestImport>
    <DefaultServices>
        <Service Name="dhclient">
            <StatelessService ServiceTypeName="dhclientType" InstanceCount="[dhclient_InstanceCount]">
                <SingletonPartition />
            </StatelessService>
        </Service>
    </DefaultServices>
</ApplicationManifest>